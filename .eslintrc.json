{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "linebreak-style": 0,
        "no-unused-expressions": 0,
        "operator-linebreak": 1,
        "import/extensions": 0,
        "no-console": 0,
        "no-useless-constructor": 0,
        "no-return-assign": 0
    }
}
